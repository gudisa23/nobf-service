					<xs:annotation>
						<xs:documentation>A single set of relationships. Line items within each set is considered an "and". Different relationships are considered an or</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="relatedTo" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Identifies a single line item this line item is related to and how it is related </xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element ref="ent:productKey"/>
										<xs:element ref="ent:optionKey" minOccurs="0"/>
										<xs:element ref="ent:relationshipType"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
