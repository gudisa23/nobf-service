	<xs:include schemaLocation="./CustomerInformation.xsd"/>
	<xs:include schemaLocation="./CATSTypes.xsd"/>
	<xs:complexType name="OrderStatusEntity">
		<xs:annotation>
			<xs:documentation>Capture various status fields of the Order or its line items</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="ent:status" minOccurs="0"/>
			<xs:element ref="ent:shippingStatus" minOccurs="0"/>
			<xs:element ref="ent:financialStatus" minOccurs="0"/>
			<xs:element ref="ent:cancelledStatus" minOccurs="0"/>
			<xs:element ref="ent:installationStatus" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="broadBandOrderId">
		<xs:annotation>
			<xs:documentation>Broadband Order Id</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string"/>
		</xs:simpleType>
	</xs:element>
	<xs:element name="equipmentType" type="xs:string">
		<xs:annotation>
			<xs:documentation>This is for the Edge Systems to validate the Mfr Model info being input - EI will ignore this</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="OrderReceiverEntity">
		<xs:annotation>
			<xs:documentation>Capture attributes of a receiver line item </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="ent:requestProvisioningFlag" minOccurs="0"/>
			<xs:element ref="ent:accessCardId" minOccurs="0"/>
			<xs:element ref="ent:receiverId" minOccurs="0"/>
			<xs:element ref="ent:manufacturer" minOccurs="0"/>
			<xs:element ref="ent:model" minOccurs="0"/>
			<xs:element ref="ent:serialNumber" minOccurs="0"/>
			<xs:element ref="ent:equipmentOwnership" minOccurs="0"/>
			<xs:element ref="ent:equipmentType" minOccurs="0"/>
			<xs:element ref="ent:location" minOccurs="0"/>
			<xs:element ref="ent:accessCardStatus" minOccurs="0"/>
			<xs:element ref="ent:accessCardType" minOccurs="0"/>
			<xs:element ref="ent:installerId" minOccurs="0"/>
			<xs:element ref="ent:chargeMirrorFeeAnnuallyFlag" minOccurs="0"/>
			<xs:element ref="ent:phoneConnectionFlag" minOccurs="0"/>
			<xs:element ref="ent:rateCeiling" minOccurs="0"/>
			<xs:element ref="ent:restrictedFlag" minOccurs="0"/>
			<xs:element ref="ent:allowIPPVFlag" minOccurs="0"/>
			<xs:element ref="ent:allowOPPVFlag" minOccurs="0"/>
			<xs:element ref="ent:mirroredFlag" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OrderAccessoryEntity">
		<xs:annotation>
			<xs:documentation>Capture attributes of an "accessory" line item</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="type">
				<xs:annotation>
					<xs:documentation>Type of the accessory (ODU, etc)</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string"/>
				</xs:simpleType>
			</xs:element>
			<xs:element ref="ent:manufacturer" minOccurs="0"/>
			<xs:element ref="ent:model" minOccurs="0"/>
			<xs:element ref="ent:quantity"/>
			<xs:element ref="ent:equipmentOwnership" minOccurs="0"/>
			<xs:element ref="ent:accessorySerialNumber" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OrderRMAEntity">
		<xs:sequence>
			<xs:element name="returnsInfo" type="ent:OrderRMAInfoEntity">
				<xs:annotation>
					<xs:documentation>Returns specific information</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="itemToBeReturned">
				<xs:annotation>
					<xs:documentation>Description and attributes of the item to be returned</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:choice>
						<xs:element name="receiver" type="ent:OrderReceiverEntity">
							<xs:annotation>
								<xs:documentation>Receiver being returned</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="accessory" type="ent:OrderAccessoryEntity">
							<xs:annotation>
								<xs:documentation>Accessory being returned</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
					<xs:attribute name="type" use="required">
						<xs:annotation>
							<xs:documentation>Type of Item being returned </xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OrderCustomerProfileEntity">
		<xs:annotation>
			<xs:documentation>Capture attributes of a Customer's Profile required to fulfill the order. </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="serviceAddress" type="ent:OrderServiceAddressEntity" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Customer's Service address</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="billingAddress" type="ent:OrderAddressEntity" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Customers billing address</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="shippingAddress" type="ent:OrderAddressEntity" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Shipping address</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="customerAddress" type="ent:OrderAddressEntity" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Customer's address if different from the Service Address - If Gift purchase, this would be the address of the Giftor</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="ent:emailAddress" minOccurs="0"/>
			<xs:element name="identificationList" minOccurs="0">
				<xs:annotation>
					<xs:documentation>List of Customer ID information</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="ent:identification" maxOccurs="unbounded"/>
					</xs:sequence>
					<xs:attribute name="count" type="xs:int"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="paymentInformation" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The Payment information that will be stored on the Customer's Profile</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="creditCard" type="ent:OrderCreditCardEntity" minOccurs="0"/>
						<xs:element name="eft" type="ent:OrderEFTEntity" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element ref="ent:makePaymentsAutomaticallyFlag" minOccurs="0"/>
			<xs:element ref="ent:accountStatus" minOccurs="0"/>
			<xs:element ref="ent:activationDate" minOccurs="0"/>
			<xs:element name="customerType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Customer Type </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string"/>
				</xs:simpleType>
			</xs:element>
			<xs:element ref="ent:accountType" minOccurs="0"/>
			<xs:element name="marketingPreferences" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Customer's Marketing Preferences</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="ent:receivePromoCallsFlag"/>
						<xs:element ref="ent:receivePromoEmailFlag"/>
						<xs:element ref="ent:sellNameToMailingListFlag"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element ref="ent:primaryOwner" minOccurs="0"/>
			<xs:element ref="ent:servicerName" minOccurs="0"/>
			<xs:element ref="ent:authorizedCreditScore" minOccurs="0"/>
			<xs:element ref="ent:proprietarySegment" minOccurs="0"/>
			<xs:element ref="ent:creditScoreRequestId" minOccurs="0"/>
			<xs:element ref="ent:customerValue" minOccurs="0"/>
			<xs:element ref="ent:camcDataCollectionConsent" minOccurs="0"/>
			<xs:element name="additionalCustomerInfo" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="ent:language" minOccurs="0"/>
						<xs:element ref="ent:welcomeLetterCode" minOccurs="0"/>
						<xs:element ref="ent:welcomeLetterNumber" minOccurs="0"/>
						<xs:element ref="ent:dateOfBirth" minOccurs="0"/>
						<xs:element ref="ent:dataReviewPassedFlag" minOccurs="0"/>
						<xs:element ref="ent:optInEligibilityFlag" minOccurs="0"/>
						<xs:element ref="ent:socialSecurityNumber" minOccurs="0"/>
						<xs:element ref="ent:ownedModelFlag" minOccurs="0"/>
						<xs:element ref="ent:yearsAtCurrentAddress" minOccurs="0"/>
						<xs:element name="previousAddressInfo" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Previous Address info should be populated with the customers previous address or a flag indicating if the previous address is out of the country</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:choice>
									<xs:element name="customerAddress">
										<xs:complexType>
											<xs:sequence>
												<xs:element ref="ent:streetNumber"/>
												<xs:element ref="ent:streetName"/>
												<xs:element ref="ent:address2" minOccurs="0"/>
												<xs:element ref="ent:apartmentNumber" minOccurs="0"/>
												<xs:element ref="ent:city"/>
												<xs:element ref="ent:state"/>
												<xs:element ref="ent:zipCode"/>
												<xs:element ref="ent:countyCode" minOccurs="0"/>
												<xs:element ref="ent:countyName" minOccurs="0"/>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
									<xs:element ref="ent:previousAddressOutOfCountryFlag"/>
								</xs:choice>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OrderAddressEntity">
		<xs:annotation>
			<xs:documentation>Captures attributes of an address</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="name" type="ent:OrderNameEntity"/>
			<xs:element ref="ent:apartmentNumber" minOccurs="0"/>
			<xs:element ref="ent:address1"/>
			<xs:element ref="ent:address2" minOccurs="0"/>
			<xs:element ref="ent:city"/>
			<xs:element ref="ent:state"/>
			<xs:element ref="ent:countyCode" minOccurs="0"/>
			<xs:element ref="ent:countyName" minOccurs="0"/>
			<xs:element ref="ent:zipCode"/>
			<xs:element name="phone" type="ent:OrderPhoneEntity" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OrderPaymentMethodEntity">
		<xs:annotation>
			<xs:documentation>Method of payment used by the customer to </xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element name="creditCard" type="ent:OrderCreditCardEntity" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Payment made by Credit Card</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="bankAccount" type="ent:OrderEFTEntity" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Payment made by EFT</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="certificateId" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Certificate Id used when method = certificate</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="prepaidCardId" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Pin Number - used when method = prepaidCard</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:choice>
		<xs:attribute name="method">
			<xs:annotation>
				<xs:documentation>creditCard, stmsBill, electronicCheck, westernUnion, inStore, notRequired etc. If creditCard, credit card detail needs to be populated. If electronicCheck, bankAccount information needs to be populated</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string"/>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="OrderAgreementsEntity">
		<xs:sequence>
			<xs:element name="credits" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OrderAgreementEntity">
		<xs:sequence>
			<xs:element name="type">
				<xs:annotation>
					<xs:documentation>Agreement Type</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string"/>
				</xs:simpleType>
			</xs:element>
			<xs:element ref="ent:rebateId"/>
			<xs:element ref="ent:provisionByDate" minOccurs="0"/>
			<xs:element ref="ent:overrideProvisionByDateCheckFlag" minOccurs="0"/>
			<xs:element ref="ent:accountId" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OrderProgrammingEntity">
		<xs:sequence>
			<xs:element ref="ent:productType"/>
			<xs:element ref="ent:productCode"/>
			<xs:element ref="ent:priceCode" minOccurs="0"/>
			<xs:element ref="ent:disconnectReason" minOccurs="0"/>
			<xs:element ref="ent:accessCardId" minOccurs="0"/>
			<xs:element ref="ent:productSourceCode" minOccurs="0"/>
			<xs:element ref="ent:purchaseType" minOccurs="0"/>
			<xs:element ref="ent:rentalExpirationTimestamp" minOccurs="0"/>
			<xs:element ref="ent:rentalDurationMinutes" minOccurs="0"/>
			<xs:element ref="ent:chargeOnlyFlag" minOccurs="0"/>
			<xs:element name="provisionDate" type="xs:dateTime" minOccurs="0"/>
			<xs:element ref="ent:accountId" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OrderFeeEntity">
		<xs:sequence>
			<xs:element ref="ent:productType"/>
			<xs:element ref="ent:productCode"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OrderBroadBandEntity">
		<xs:sequence>
			<xs:element ref="ent:brandedFlag"/>
			<xs:element ref="ent:productType" minOccurs="0"/>
			<xs:element ref="ent:productCode" minOccurs="0"/>
			<xs:element ref="ent:priceCode" minOccurs="0"/>
			<xs:element ref="ent:trackingId" minOccurs="0"/>
			<xs:element ref="ent:brandedProviderName" minOccurs="0"/>
			<xs:element ref="ent:brandedProductName" minOccurs="0"/>
			<xs:element ref="ent:broadbandProductCode"/>
			<xs:element name="clientAuthenticationList" type="ent:OrderClientAuthenticationListEntity" minOccurs="0"/>
			<xs:element name="productOptionsList" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="productOption" maxOccurs="unbounded">
							<xs:complexType>
								<xs:sequence>
									<xs:element ref="ent:optionId"/>
									<xs:element name="optionValueList" minOccurs="0">
										<xs:complexType>
											<xs:sequence>
												<xs:element ref="ent:valueId" maxOccurs="unbounded"/>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="loginInformation" type="ent:LoginEntity" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OrderPriceEntity">
		<xs:sequence>
			<xs:element ref="ent:totalAmount"/>
			<xs:element ref="ent:subTotalAmount"/>
			<xs:element ref="ent:taxAmount"/>
			<xs:element ref="ent:shippingAmount"/>
			<xs:element name="recurringPriceList" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="recurringPrice" type="ent:OrderRecurringPriceEntity" maxOccurs="unbounded">
							<xs:annotation>
								<xs:documentation>Recurring Amount that will be charged to the Customer's Account. If the initialRecurringAmount section is populated, this will indicate the "roll-to" price</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OrderRMAInfoEntity">
		<xs:sequence>
			<xs:element ref="ent:rmaType" minOccurs="0"/>
			<xs:element ref="ent:genericMaterial" minOccurs="0"/>
			<xs:element name="reasonCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Reason the receiver is being returned </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string"/>
				</xs:simpleType>
			</xs:element>
			<xs:element ref="ent:frequency" minOccurs="0"/>
			<xs:element ref="ent:returnToASCAllowedFlag" minOccurs="0"/>
			<xs:element ref="ent:exchangeAllowedFlag" minOccurs="0"/>
			<xs:element ref="ent:rmaReturnLabelShipMethod" minOccurs="0"/>
			<xs:element name="noReturnFlag" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>A Return is not expected for this RMA</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OrderLineItemEntity">
		<xs:complexContent>
			<xs:extension base="ent:BasicEntity">
				<xs:sequence>
					<xs:element name="orderType">
						<xs:annotation>
							<xs:documentation>Line Item Order Type</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string"/>
						</xs:simpleType>
					</xs:element>
					<xs:element ref="ent:lineItemType"/>
					<xs:element ref="ent:action"/>
					<xs:element ref="ent:lineItemIdentifier" minOccurs="0"/>
					<xs:element name="relationshipList" type="ent:OrderLineItemRelationshipListEntity" minOccurs="0">
						<xs:annotation>
							<xs:documentation>List of line item relationships</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="statusList" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Status of various activities performed on this line item </xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="activity" type="ent:OrderActivityEntity" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation>An Activity performed on this line item</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="price" type="ent:OrderPriceEntity" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Line item price</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element ref="ent:shippingMethod" minOccurs="0"/>
					<xs:element ref="ent:effectiveDate" minOccurs="0"/>
					<xs:element ref="ent:queueWhenProvisionerDownFlag" minOccurs="0"/>
					<xs:element ref="ent:reshipFlag" minOccurs="0"/>
					<xs:element name="lineItemDetail">
						<xs:complexType>
							<xs:choice>
								<xs:element name="offer" type="ent:OrderOfferEntity">
									<xs:annotation>
										<xs:documentation>Attributes of an offer line item</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="offerDetail" type="ent:OrderLineItemOfferDetailEntity">
									<xs:annotation>
										<xs:documentation>Offer Detail information</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="agreement" type="ent:OrderAgreementEntity">
									<xs:annotation>
										<xs:documentation>Attributes of an agreement line item</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="receiver" type="ent:OrderReceiverEntity">
									<xs:annotation>
										<xs:documentation>Attributes of a "receiver" line item</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="programming" type="ent:OrderProgrammingEntity">
									<xs:annotation>
										<xs:documentation>Programming product attributes</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="fee" type="ent:OrderFeeEntity">
									<xs:annotation>
										<xs:documentation>Commands that trigger various activities in the enterprise e.g., B Codes</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="adjustment" type="ent:OrderAdjustmentLineItemEntity">
									<xs:annotation>
										<xs:documentation>Attributes of an Adjustment Line item</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="recording" type="ent:RecordingEntity">
									<xs:annotation>
										<xs:documentation>Used for Broadband Video and Remote Booking (Mobile Planner)</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="accessory" type="ent:OrderAccessoryEntity">
									<xs:annotation>
										<xs:documentation>Attributes of accessories  ordered</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="broadband" type="ent:OrderBroadBandEntity">
									<xs:annotation>
										<xs:documentation>Contains info about Broadband internet provisioning</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="comment" type="ent:OrderCommentEntity">
									<xs:annotation>
										<xs:documentation>Attributes of a Comment being added</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="rma" type="ent:OrderRMAEntity">
									<xs:annotation>
										<xs:documentation>RMA Information</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="installation" type="ent:OrderLineItemInstallationEntity">
									<xs:annotation>
										<xs:documentation>Installation Information</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="virtual">
									<xs:annotation>
										<xs:documentation>This can be used for Line Items that dont require any detail. </xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:choice>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="visibleFlag" type="ent:BooleanEntity" use="required">
					<xs:annotation>
						<xs:documentation>Is this line item visible to the customer on a User Interface</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="provisionableFlag" type="ent:BooleanEntity" use="required">
					<xs:annotation>
						<xs:documentation>Indicates if this line item can be provisioned - For e.g., a line item that describes a remote (accessory) will have this flag set to false. </xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="addedByRuleFlag" type="ent:BooleanEntity">
					<xs:annotation>
						<xs:documentation>Was this line item added by a rule or was this part of the original Customer Order</xs:documentation>
					</xs:annotation>
