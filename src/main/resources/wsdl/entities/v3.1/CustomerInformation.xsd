								<xs:element name="provideAdditionalAccountInfo" type="xs:boolean" minOccurs="0"/>
								<xs:element name="cryptoInfo" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Provides ability to return customer cryptographic information</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:sequence>
											<xs:element name="provideCryptoInfoFlag" type="xs:boolean" minOccurs="0">
