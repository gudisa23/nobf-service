<xs:schema xmlns:ent="http://ei.directv.com/schemas/entities/v3_1" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://ei.directv.com/schemas/entities/v3_1" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:include schemaLocation="./TargetInformation.xsd"/>
	<xs:attributeGroup name="profileRequestAttribGroup">
		<xs:annotation>
			<xs:documentation>Group of attributes that identify the filters to be applied for profile requests</xs:documentation>
		</xs:annotation>
		<xs:attribute name="customerAccountInfoFlag" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>Return customer account information </xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="receiverInfoFlag" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>Return receiver information</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="recurringCreditsFlag" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>Return Recurring credit information, which will show any active or inactive recurring credits.  </xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="provideInflightTransactionInfoFlag" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>Return Inflight transaction info.  </xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="provideCustomerValueFlag" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>Return customer value.  </xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="provideCustomerAgreementDetailFlag" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>Return customer agreement detail </xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="provideCustomerCreditTotalsFlag" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>Return customer credit totalsl </xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:simpleType name="ProgrammingStatusFilterEntity">
		<xs:annotation>
			<xs:documentation>The status of the programming for filtering </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="pending"/>
			<xs:enumeration value="active"/>
			<xs:enumeration value="disconnected"/>
			<xs:enumeration value="all"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="RecurringCreditStatusFilterEntity">
		<xs:annotation>
			<xs:documentation>The status of the recurring credits for filtering </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="active"/>
			<xs:enumeration value="inactive"/>
			<xs:enumeration value="all"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="RecurringAdjustmentStatusFilterEntity">
		<xs:annotation>
			<xs:documentation>The status of the recurring Adjustments for filtering </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="active"/>
			<xs:enumeration value="inactive"/>
			<xs:enumeration value="all"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ReceiverStatusFilterEntity">
		<xs:annotation>
			<xs:documentation>The status of the receiver for filtering </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="pending"/>
			<xs:enumeration value="active"/>
			<xs:enumeration value="disconnected"/>
			<xs:enumeration value="all"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="AccountEntity">
		<xs:annotation>
			<xs:documentation>A container to store the common elements of a Subscriber and Dealer accounts</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ent:BasicAccountInfoEntity">
				<xs:sequence>
					<xs:element ref="ent:accountHierarchy" minOccurs="0"/>
					<xs:element ref="ent:accountStatusDetails" minOccurs="0"/>
					<xs:element ref="ent:commentList" minOccurs="0"/>
					<xs:element ref="ent:targetList" minOccurs="0"/>
					<xs:element ref="ent:servicerFlag" minOccurs="0"/>
					<xs:element ref="ent:alternateAccountFlag" minOccurs="0"/>
					<xs:element ref="ent:alternateServicerFlag" minOccurs="0"/>
					<xs:element ref="ent:servicer" minOccurs="0"/>
					<xs:element ref="ent:billToBillingAddressFlag" minOccurs="0"/>
					<xs:element ref="ent:billingAddress" minOccurs="0"/>
					<xs:element ref="ent:accountFinancialInfo" minOccurs="0"/>
					<xs:element ref="ent:financialActivities" minOccurs="0"/>
					<xs:element ref="ent:dealerId" minOccurs="0"/>
					<xs:element ref="ent:installerId" minOccurs="0"/>
					<xs:element ref="ent:accountSubscriptionInfo" minOccurs="0"/>
					<xs:element ref="ent:serviceCallInfoList" minOccurs="0"/>
					<xs:element ref="ent:reinstatementResults" minOccurs="0"/>
					<xs:element ref="ent:pendingTransactionList" minOccurs="0"/>
					<xs:element ref="ent:dealerAccountType" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="AccountListEntity">
		<xs:complexContent>
			<xs:extension base="ent:ListEntity">
				<xs:sequence>
					<xs:element ref="ent:account" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="AccountFinancialInfoEntity">
		<xs:annotation>
			<xs:documentation>Stores minimal financial information</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ent:BasicEntity">
				<xs:sequence>
					<xs:element ref="ent:accountBalance" minOccurs="0"/>
					<xs:element ref="ent:dealerFlag" minOccurs="0"/>
					<xs:element ref="ent:lastBillAmount" minOccurs="0"/>
					<xs:element ref="ent:lastBillDate" minOccurs="0"/>
					<xs:element ref="ent:lastBillDueDate" minOccurs="0"/>
					<xs:element ref="ent:lastBillPaymentDate" minOccurs="0"/>
					<xs:element ref="ent:pastDueAmount" minOccurs="0"/>
					<xs:element ref="ent:pastDueFlag" minOccurs="0"/>
					<xs:element ref="ent:lastPaymentAmount" minOccurs="0"/>
					<xs:element ref="ent:paymentConfirmationNumber" minOccurs="0"/>
					<xs:element ref="ent:writeOffAmount" minOccurs="0"/>
					<xs:element ref="ent:writeOffDate" minOccurs="0"/>
					<xs:element ref="ent:accountPastDueThreshold" minOccurs="0"/>
					<xs:element ref="ent:endOfMonthBalance" minOccurs="0"/>
					<xs:element ref="ent:billingCycleDay" minOccurs="0"/>
					<xs:element ref="ent:serviceCycleDay" minOccurs="0"/>
					<xs:element ref="ent:oppvLimit" minOccurs="0"/>
					<xs:element ref="ent:nonPaymentCode" minOccurs="0"/>
					<xs:element ref="ent:statementDeliveryOption" minOccurs="0"/>
					<xs:element ref="ent:paymentMethod" minOccurs="0"/>
					<xs:element ref="ent:paymentMethodList" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="AccountHierarchyInfoEntity">
		<xs:annotation>
			<xs:documentation>Information about the Account's Hierarchy </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="ent:messageList" minOccurs="0"/>
			<xs:element name="parentAccountInfo" type="ent:HierarchyDefnEntity" minOccurs="0"/>
			<xs:element name="childAccountList" minOccurs="0">
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="ent:ListEntity">
							<xs:sequence>
								<xs:element name="childAccountInfo" type="ent:HierarchyDefnEntity" minOccurs="0" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AccountProgrammingInfoEntity">
		<xs:annotation>
			<xs:documentation>Stores information relating to service availability on an account</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ent:BasicEntity">
				<xs:sequence>
					<xs:element ref="ent:cutoffLevel" minOccurs="0"/>
					<xs:element ref="ent:cutoffDate" minOccurs="0"/>
					<xs:element ref="ent:extendCutoffDays" minOccurs="0"/>
					<xs:element ref="ent:beginSuspensionDate" minOccurs="0"/>
					<xs:element ref="ent:endSuspensionDate" minOccurs="0"/>
					<xs:element ref="ent:disconnectDate" minOccurs="0"/>
					<xs:element ref="ent:disconnectType" minOccurs="0"/>
					<xs:element ref="ent:disconnectReason" minOccurs="0"/>
					<xs:element ref="ent:reinstateServicesAutomaticallyFlag" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="AccountSubscriptionInfoEntity">
		<xs:complexContent>
			<xs:extension base="ent:BasicEntity">
				<xs:sequence>
					<xs:element ref="ent:hasCableTVFlag" minOccurs="0"/>
					<xs:element ref="ent:activationDate" minOccurs="0"/>
					<xs:element ref="ent:createdByDealerFlag" minOccurs="0"/>
					<xs:element ref="ent:servicedByDealerFlag" minOccurs="0"/>
					<xs:element ref="ent:salesAgentId" minOccurs="0"/>
					<xs:element ref="ent:salesAgentName" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="BaseCustomerEntity">
		<xs:annotation>
			<xs:documentation>Basic Customer information, common to both Subscribers and Dealers</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ent:BasicEntity">
				<xs:sequence>
					<xs:element ref="ent:customerType" minOccurs="0"/>
					<xs:element ref="ent:customerStatus" minOccurs="0"/>
					<xs:element ref="ent:customerId" minOccurs="0"/>
					<xs:element ref="ent:customerSinceDate" minOccurs="0"/>
					<xs:element ref="ent:language" minOccurs="0"/>
					<xs:element ref="ent:languageCode" minOccurs="0"/>
					<xs:element ref="ent:contactInformationList" minOccurs="0"/>
					<xs:element ref="ent:creditSegment" minOccurs="0"/>
					<xs:element ref="ent:creditSegmentCalculatedOnDate" minOccurs="0"/>
					<xs:element ref="ent:proprietorySegment" minOccurs="0"/>
					<xs:element ref="ent:involuntaryRiskCode" minOccurs="0"/>
					<xs:element ref="ent:ownedModelFlag" minOccurs="0"/>
					<xs:element ref="ent:telcoNonpayStatus" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="BasicAccountInfoEntity">
		<xs:annotation>
			<xs:documentation>Stores minimal account information common to both subscriber and dealer</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ent:BasicEntity">
				<xs:sequence>
					<xs:element ref="ent:accountId" minOccurs="0"/>
					<xs:element ref="ent:accountIndicator" minOccurs="0"/>
					<xs:element ref="ent:accountType" minOccurs="0"/>
					<xs:element ref="ent:accountStatus" minOccurs="0"/>
					<xs:element ref="ent:password" minOccurs="0"/>
					<xs:element ref="ent:creationDate" minOccurs="0"/>
					<xs:element ref="ent:commissionStartDate" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="CommentEntity">
		<xs:complexContent>
			<xs:extension base="ent:BasicEntity">
				<xs:sequence>
					<xs:element ref="ent:commentAddedOnDateTime" minOccurs="0"/>
					<xs:element ref="ent:commentCode" minOccurs="0"/>
					<xs:element ref="ent:commentType" minOccurs="0"/>
					<xs:element ref="ent:commentText" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="CommentListEntity">
		<xs:annotation>
			<xs:documentation>A Container to store a list of comments</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ent:ListEntity">
				<xs:sequence>
					<xs:element ref="ent:comment" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="CommitmentInformationEntity">
		<xs:sequence>
			<xs:element ref="ent:commitmentExpirationDate" minOccurs="0"/>
			<xs:element ref="ent:commitmentValue" minOccurs="0"/>
			<xs:element ref="ent:commitmentDescription" minOccurs="0"/>
			<xs:element ref="ent:commitmentDuration" minOccurs="0"/>
			<xs:element ref="ent:commitmentStatus" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="agreementInformationList">
		<xs:annotation>
			<xs:documentation>A list of customer agreement information</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="agreement" type="ent:AgreementInformationEntity" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="AgreementInformationEntity">
		<xs:sequence>
			<xs:element ref="ent:agreementId" minOccurs="0"/>
			<xs:element ref="ent:agreementStartDate" minOccurs="0"/>
			<xs:element ref="ent:agreementEndDate" minOccurs="0"/>
			<xs:element ref="ent:orderDate" minOccurs="0"/>
			<xs:element ref="ent:agreementDescription" minOccurs="0"/>
			<xs:element ref="ent:agreementIsIncrementalFlag" minOccurs="0"/>
			<xs:element ref="ent:agreementStatus" minOccurs="0"/>
			<xs:element ref="ent:nonComplianceReason" minOccurs="0"/>
			<xs:element ref="ent:agreementPriceCode" minOccurs="0"/>
			<xs:element ref="ent:numberOfEnrollments" minOccurs="0"/>
			<xs:element name="offerId" type="xs:string" minOccurs="0"/>
			<xs:element ref="ent:offerType" minOccurs="0"/>
			<xs:element name="commitmentInformation" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Commitment Information</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element ref="ent:commitmentValue" minOccurs="0"/>
						<xs:element ref="ent:commitmentDuration" minOccurs="0"/>
						<xs:element ref="ent:commitmentStatus" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element ref="ent:customData" minOccurs="0"/>
			<xs:element name="agreementType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Agreement  information</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:choice>
						<xs:element ref="ent:hardwareAgreement" minOccurs="0"/>
						<xs:element ref="ent:programmingAgreement" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Programming agreement information</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:choice>
				</xs:complexType>
			</xs:element>
			<xs:element ref="ent:agreementStatusEffectiveDate" minOccurs="0"/>
			<xs:element ref="ent:eventDetailsList" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="eventDetailsList">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="eventDetails" type="ent:EventDetailsEntity" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="EventDetailsEntity">
		<xs:sequence>
			<xs:element ref="ent:agreementEventName" minOccurs="0"/>
			<xs:element ref="ent:agreementEventStatus" minOccurs="0"/>
			<xs:element ref="ent:agreementEventDateTime" minOccurs="0"/>
			<xs:element ref="ent:redemptionMethod" minOccurs="0"/>
			<xs:element ref="ent:entryMethod" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="hardwareAgreement" type="ent:HardwareAgreementEntity">
		<xs:annotation>
			<xs:documentation>Hardware agreement information</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="HardwareAgreementEntity">
		<xs:sequence>
			<xs:element ref="ent:dominateCommitmentFlag" minOccurs="0"/>
			<xs:element ref="ent:manufacturer" minOccurs="0"/>
			<xs:element ref="ent:model" minOccurs="0"/>
			<xs:element ref="ent:serialNumber" minOccurs="0"/>
			<xs:element ref="ent:receiverId" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ProgrammingAgreementEntity">
		<xs:sequence>
			<xs:element ref="ent:productType" minOccurs="0"/>
			<xs:element ref="ent:productCode" minOccurs="0"/>
			<xs:element ref="ent:recurringCreditAmount" minOccurs="0"/>
			<xs:element ref="ent:recurringCreditCount" minOccurs="0"/>
			<xs:element ref="ent:recurringCreditsIssued" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="programmingAgreement" type="ent:ProgrammingAgreementEntity">
		<xs:annotation>
			<xs:documentation>Programming agreement information</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="CustomerEntity">
		<xs:complexContent>
