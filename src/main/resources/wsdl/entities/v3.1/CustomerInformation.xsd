										<xs:annotation>
											<xs:documentation>Return receiver information for this access card</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:choice>
								<xs:element name="previousAccessCardInfoFlag" type="xs:boolean" minOccurs="0">
