										<xs:sequence>
											<xs:element name="getNextReceiversFlag" type="xs:boolean" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Return access card information for next set of receivers</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="lastAccessCardId" type="ent:AccessCardIdEntity" minOccurs="0">
												<xs:annotation>
													<xs:documentation>Last access card id. To be used with getNextTargets for paging</xs:documentation>
												</xs:annotation>
											</xs:element>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
								<xs:element name="programmingInformationFilters" minOccurs="0">
									<xs:annotation>
										<xs:documentation>Filters applied while returning programming information</xs:documentation>
									</xs:annotation>
									<xs:complexType>
